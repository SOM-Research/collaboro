<?xml version="1.0" encoding="ASCII"?>
<notation:Definition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:notation="http://atlanmod.fr/collaboro/notation">
  <elements
      xsi:type="notation:Composite"
      id="Network">
    <subElements
        xsi:type="notation:SyntaxOf"
        id="Network.vehicules"/>
    <subElements
        xsi:type="notation:SyntaxOf"
        id="Network.routes"/>
    <subElements
        xsi:type="notation:SyntaxOf"
        id="Network.stops"/>
  </elements>
  <elements
      xsi:type="notation:Composite"
      id="Tram">
    <subElements
        xsi:type="notation:Image"
        id="ImageBus"
        fill="blue"/>
    <subElements
        xsi:type="notation:Label"
        id="LabelTramId"
        y="20"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Tram.id"
          separator=""/>
    </subElements>
    <subElements
        xsi:type="notation:Line"
        id="Tram.follows"
        fill="black"/>
  </elements>
  <elements
      xsi:type="notation:Composite"
      id="Bus">
    <subElements
        xsi:type="notation:Image"
        id="ImageBus"
        fill="blue"/>
    <subElements
        xsi:type="notation:Label"
        id="LabelBusId"
        y="20"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Bus.id"/>
    </subElements>
    <subElements
        xsi:type="notation:Line"
        id="Bus.follows"
        fill="black"/>
  </elements>
  <elements
      xsi:type="notation:Composite"
      id="Route">
    <subElements
        xsi:type="notation:SyntaxOf"
        id="Route"/>
    <subElements
        xsi:type="notation:Line"
        id="Route.stops"/>
    <subElements
        xsi:type="notation:SyntaxOf"
        id="Stop"/>
  </elements>
  <elements
      xsi:type="notation:Composite"
      id="Stop">
    <subElements
        xsi:type="notation:Image"
        id="ImageStop"
        fill="yellow"/>
    <subElements
        xsi:type="notation:Label"
        id=""
        y="20"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Stop.id"/>
    </subElements>
    <subElements
        xsi:type="notation:Label"
        id=""
        y="40"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Stop.lat"/>
    </subElements>
    <subElements
        xsi:type="notation:Label"
        id=""
        y="60"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Stop.lon"/>
    </subElements>
    <subElements
        xsi:type="notation:Label"
        id=""
        y="70"
        width="100">
      <text
          xsi:type="notation:AttributeValue"
          id="Stop.handicap"/>
    </subElements>
  </elements>
</notation:Definition>
